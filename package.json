    "@types/react-redux": "5.0.8",
    "@types/react-tether": "0.5.2",
    "@types/underscore": "1.8.3",
    "@types/underscore.string": "0.0.31",
    "@types/webpack-env": "1.13.2",
    "autoprefixer": "7.1.6",
    "bithound": "1.7.0",
    "chosen-js": "1.8.2",
    "classnames": "2.2.5",
    "codecov": "3.0.0",
